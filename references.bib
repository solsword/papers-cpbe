@article{kallio2011least,
  title={At Least Nine Ways to Play: Approaching Gamer Mentalities},
  author={Kallio, Kirsi Pauliina and MÃ¤yrÃ¤, Frans and Kaipainen, Kirsikka}, 
  volume={6}, 
  number={4}, 
  pages={327-353}, 
  year={2011}, 
  doi={10.1177/1555412010391089}, 
  abstract ={Do digital games and play mean the same things for different people? This article presents the results of a 3-year study in which we sought for new ways to approach digital games cultures and playing practices. First, the authors present the research process in brief and emphasize the importance of merging different kinds of methods and materials in the study of games cultures. Second, the authors introduce a gaming mentality heuristics that is not dedicated to a certain domain or genre of games, addressing light casual and light social gaming motivations as well as more dedicated ones in a joint framework. The analysis reveals that, in contrast to common belief, the majority of digital gaming takes place between ``casual relaxing'' and ``committed entertaining,'' where the multiplicity of experiences, feelings, and understandings that people have about their playing and digital games is wide ranging. Digital gaming is thus found to be a multifaceted social and cultural phenomenon that can be understood, practiced, and used in various ways.}, 
  URL={http://gac.sagepub.com/content/6/4/327.abstract}, 
  eprint={http://gac.sagepub.com/content/6/4/327.full.pdf+html}, 
  journal={Games and Culture} 
}

